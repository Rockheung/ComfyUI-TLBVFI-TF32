  "workflow_info": {
    "name": "TLBVFI Chunk-Based Interpolation Example",
    "description": "Example workflow demonstrating chunk-based video frame interpolation using TLBVFI custom nodes (excluding chunk_processor and display_text)",
    "author": "ComfyUI-TLBVFI-TF32",
    "nodes_used": [
      "VHS_LoadVideo",
      "TLBVFI_FramePairSlicer",
      "TLBVFI_Interpolator",
      "TLBVFI_ChunkVideoSaver",
      "TLBVFI_VideoConcatenator"
    ],
    "workflow_steps": [
      "1. Load video using VHS_LoadVideo",
      "2. Slice video into frame pairs using FramePairSlicer",
      "3. Interpolate each frame pair using Interpolator (8x interpolation with times_to_interpolate=3)",
      "4. Save interpolated frames as video chunks using ChunkVideoSaver",
      "5. Concatenate all chunks into final video using VideoConcatenator"
    ],
    "notes": [
      "This workflow processes one frame pair at a time for memory efficiency",
      "For processing all pairs, use a loop mechanism to iterate pair_index from 0 to total_pairs-1",
      "The shared_model output is connected back to its own input to avoid reloading the model",
      "chunk_id is automatically connected from pair_index for sequential chunk numbering",
      "The final video is saved without re-encoding using FFmpeg concat demuxer"
    ]
  }
